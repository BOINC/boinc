{
    "name": "boinc-manager",
    "dependencies":
    [
        {
          "name": "curl",
          "features": ["openssl"],
          "default-features": false
        },
        {
          "name": "openssl",
          "features": ["ssl3", "weak-ssl-ciphers"],
          "default-features": false
        },
        {
            "name": "gtk3",
            "default-features": false,
            "features": ["core"]
        },
        {
          "name": "wxwidgets",
          "default-features": false
        },
        "freetype",
        "librsvg",
        {
          "name": "gdk-pixbuf",
          "features": ["jpeg", "png", "svg", "tiff", "others"],
          "default-features": false
        },
        {
          "name": "libzip",
          "default-features": false
        },
        "libnotify",
        "xcb-util",
        "pixbufloader-svg"
    ]
}
