<?xml version="1.0" encoding="UTF-8"?>
<skin>
	<en>
		<simple>
			<background_image>
				<!-- Background Image for the Simple GUI-->
				<!--	410x540 -->
				<image>
					<imagesrc>graphic/background_image.png</imagesrc>
					<background_color>236:233:233</background_color>
				</image>
			</background_image>
			<spacer_image>
				<image>
					<imagesrc>graphic/spacer_image.png</imagesrc>
				</image>
			</spacer_image>
			<static_line_color>255:255:255</static_line_color>
			<state_indicator_background_image>
